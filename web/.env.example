# API Configuration
VITE_API_BASE_URL=http://localhost:8084/api/v1
VITE_WEBSOCKET_URL=ws://localhost:8084/api/v1/chat

# Authentication Configuration
VITE_AUTH_BASE_URL=http://localhost:8084/api/v1/auth
VITE_GOOGLE_CLIENT_ID=your-google-client-id-from-console.cloud.google.com
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret-from-console.cloud.google.com
BETTER_AUTH_SECRET=generate-a-secure-secret-key-for-better-auth

# GitHub Integration (for repository creation and deployment)
GITHUB_TOKEN=your-github-personal-access-token
GITHUB_CLIENT_ID=your-github-oauth-app-client-id
GITHUB_CLIENT_SECRET=your-github-oauth-app-client-secret

# Vercel Integration (for deployment)
VERCEL_TOKEN=your-vercel-api-token
VERCEL_TEAM_ID=your-vercel-team-id-optional

# LLM Provider Configuration
VITE_DEFAULT_LLM_PROVIDER=bedrock
VITE_DEFAULT_MODEL=anthropic.claude-3-5-sonnet-20240620-v1:0

# Available Models for UI
VITE_OLLAMA_MODELS=qwen2.5:1.5b,cogito:8b,gemma3:1b,qwen3:0.6b
VITE_BEDROCK_MODELS=anthropic.claude-3-5-sonnet-20241022-v2:0,anthropic.claude-3-haiku-20240307-v1:0,anthropic.claude-3-opus-20240229-v1:0

# Development Configuration
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug
